{
  "compilerOptions": {
    "types": ["jest", "node"],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
    "resolveJsonModule": true,
    "skipLibCheck" : true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
    }
  },
  "include" : [
      "src-with-css-loader/**/*"
  ],
  "exclude" : [
      "src-with-css-loader/**/tests/**/*",
      "node_modules",
  ]
}
